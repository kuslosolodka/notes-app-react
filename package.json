{
    "name": "notes-app-react",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint:prettify": "prettier --check \"**/*.{ts,tsx,json,md,html}\"",
        "lint:css": "npx stylelint \"src/**/*.{tsx,ts}\"",
        "lint:type": "npx tsc --noEmit",
        "lint:js": "npx eslint \"src/**/*.{ts,tsx}\"",
        "lint:all": "npm run lint:prettify && npm run lint:css && npm run lint:type && npm run lint:js",
        "preview": "vite preview",
        "prettify": "prettier --write \"**/*.{ts,tsx,json,md,scss,html,js,cjs}\"",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "devDependencies": {
        "@commitlint/cli": "17.6.7",
        "@commitlint/config-conventional": "17.6.7",
        "@storybook/addon-essentials": "7.2.0",
        "@storybook/addon-interactions": "7.2.0",
        "@storybook/addon-links": "7.2.0",
        "@storybook/blocks": "7.2.0",
        "@storybook/react": "7.2.0",
        "@storybook/react-vite": "7.2.0",
        "@storybook/testing-library": "0.2.0",
        "@types/react": "18.2.17",
        "@types/react-dom": "18.2.7",
        "@vitejs/plugin-react-swc": "3.3.2",
        "eslint": "8.46.0",
        "eslint-import-resolver-typescript": "3.5.5",
        "eslint-plugin-import": "2.28.0",
        "eslint-plugin-jsx-a11y": "6.7.1",
        "eslint-plugin-n": "16.0.1",
        "eslint-plugin-promise": "6.1.1",
        "eslint-plugin-react": "7.33.1",
        "eslint-plugin-react-refresh": "0.4.3",
        "eslint-plugin-simple-import-sort": "10.0.0",
        "eslint-plugin-sonar": "0.12.0",
        "eslint-plugin-storybook": "0.6.13",
        "eslint-plugin-unicorn": "48.0.1",
        "lint-staged": "13.2.3",
        "postcss-styled-syntax": "0.4.0",
        "prettier": "3.0.0",
        "simple-git-hooks": "2.9.0",
        "storybook": "7.2.0",
        "stylelint": "15.10.2",
        "stylelint-config-standard": "34.0.0",
        "typescript": "5.1.6",
        "vite": "4.4.7"
    },
    "dependencies": {
        "@hookform/resolvers": "3.1.1",
        "@storybook/addon-styling": "1.3.5",
        "@typescript-eslint/eslint-plugin": "6.2.0",
        "@typescript-eslint/parser": "6.0.0",
        "eslint-config-prettier": "8.9.0",
        "eslint-plugin-react-hooks": "4.6.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "7.45.2",
        "simple-zustand-devtools": "1.1.0",
        "styled-components": "6.0.5",
        "zod": "3.21.4",
        "zustand": "4.3.9"
    },
    "simple-git-hooks": {
        "commit-msg": "npx commitlint --edit $1",
        "pre-commit": "npx lint-staged"
    }
}
